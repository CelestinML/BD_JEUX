CREATE OR REPLACE PROCEDURE AJOUTFORFAIT(code IN CHAR, nom IN VARCHAR2 DEFAULT 'Forfait', desc_param IN VARCHAR2, prix IN NUMBER) AS 

BEGIN
    
    INSERT INTO FORFAIT VALUES(code, nom, desc_param);
    INSERT INTO PERIODE(DATEDEBUT, DATEFIN, PRIX, CODEFORFAIT) VALUES(SYSDATE(), ADD_MONTHS(SYSDATE(), 12), prix, code);
    
END AJOUTFORFAIT;